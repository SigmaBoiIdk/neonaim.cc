local function GetDictionaryLength(dictionary)
	local FlatIdent_66556 = 0;
	local count;
	while true do
		if (FlatIdent_66556 == 0) then
			count = 0;
			for _ in pairs(dictionary) do
				count += 1
			end
			FlatIdent_66556 = 1;
		end
		if (FlatIdent_66556 == 1) then
			return count;
		end
	end
end
