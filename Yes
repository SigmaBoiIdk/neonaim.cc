			break;
		end
		if (FlatIdent_6A83E == 0) then
			FrameCounter += 1
			if ((tick() - FrameTimer) >= 1) then
				local FlatIdent_12544 = 0;
				while true do
					if (FlatIdent_12544 == 0) then
						FPS = FrameCounter;
						FrameTimer = tick();
						FlatIdent_12544 = 1;
					end
					if (FlatIdent_12544 == 1) then
						FrameCounter = 0;
						break;
					end
				end
			end
			FlatIdent_6A83E = 1;
		end
	end
end);
